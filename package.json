{
  "name": "@mohammad_obed/typescript-common",
  "version": "0.0.6",
  "description": "My TypeScript Library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./constants": {
      "types": "./dist/constants/index.d.mts",
      "default": "./dist/constants/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.mts",
      "default": "./dist/utils/index.mjs"
    }
  },
  "scripts": {
    "build": "npx moc-build",
    "all": "npx moc-build-all",
    "size": "npx moc-size",
    "run": "npx moc-run dev.ts",
    "check-types": "npx moc-check-types",
    "lint": "npx moc-lint"
  },
  "keywords": [
    "typescript",
    "library"
  ],
  "author": "mohammad_obed",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MohammadAObed/typescript-common"
  },
  "bugs": {
    "url": "https://github.com/MohammadAObed/typescript-common/issues"
  },
  "homepage": "https://github.com/MohammadAObed/typescript-common#readme",
  "devDependencies": {
    "@mohammad_obed/config": "^0.0.20",
    "typescript": "^5.9.2"
  },
  "prettier": "@mohammad_obed/config/prettier.config",
  "size-limit": [
    {
      "path": "dist",
      "limit": "2 kB"
    }
  ]
}
